/*
 * this test is used to test the lexer.
 */
import print from "std"

class Test1 {
    var int a = 10;
    method Test1 init(self, int value) {
        self.a = value;
        return self;
    }
    method int get_a(self) {
        return self.a;
    }
    method void set_a(self, int value) {
        self.a = value;
    }
}

// entry point
func int main() {
    var Test1 t = Test1.init(5);
    print(t.get_a());
    t.set_a(20);
    print(t.get_a());
    var float _abc123 = 30.5;
    var int _def456 = 15;
    return 0;
}
